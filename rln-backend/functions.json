{
  "functions": {
    "getEndpoints": {
      "handler": "dist/handler.getEndpoints",
      "events": [
        {
          "http": {
            "path": "endpoints",
            "method": "get",
            "cors": true
          }
        }
      ]
    },
    "postEndpoints": {
      "handler": "dist/handler.postEndpoints",
      "events": [
        {
          "http": {
            "path": "endpoints",
            "method": "post",
            "cors": true
          }
        }
      ]
    }
  }
}
